{
  "name": "@infra-x/eslint-config",
  "type": "module",
  "version": "0.1.3",
  "description": "A shared ESLint config for infra-x projects.",
  "author": "infra-x",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oNo500/infra-code.git",
    "directory": "packages/eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.config.json --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.13.0",
    "@eslint/compat": "1.4.1",
    "@eslint/core": "0.17.0",
    "@eslint/js": "^9.39.3",
    "@next/eslint-plugin-next": "^16.1.6",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@vitest/eslint-plugin": "^1.6.9",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-better-tailwindcss": "^4.3.1",
    "eslint-plugin-boundaries": "^5.4.0",
    "eslint-plugin-depend": "^1.4.0",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jsdoc": "^61.7.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.64.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-storybook": "^10.2.12",
    "eslint-plugin-unicorn": "^57.0.0",
    "typescript-eslint": "^8.56.1"
  },
  "peerDependencies": {
    "eslint": "^9.39.2",
    "globals": "16.5.0",
    "jiti": "2.6.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@infra-x/typescript-config": "workspace:*",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/node": "^25.3.1",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.3",
    "globals": "16.5.0",
    "jiti": "2.6.1",
    "prettier": "^3.8.1",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
